{
  "presets": ["env", "react"],
  "plugins": [
    "transform-class-properties",
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src/**"
        ],
        "extensions": [".js", ".jsx"]
      }
    ]
  ],
  "ignore": ["node_modules"]
}
