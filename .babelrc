{
  "presets": [
    [
      "env", {
        "modules": false,
        "useBuiltIns": "entry"
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }],
    [
      "module-resolver",
      {
        "alias": {
          "src": "./src",
          "lib": "./src/lib",
          "services": "./src/services",
          "plugins": "./src/plugins"
        },
        "extensions": [".js", ".jsx"]
      }
    ]
  ]
}
