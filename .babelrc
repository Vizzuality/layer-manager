{
  "presets": [
    [
      "env", {
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-class-properties",
    [
      "module-resolver",
      {
        "alias": {
          "src": "./src",
          "lib": "./src/lib",
          "services": "./src/services",
          "plugins": "./src/plugins"
        },
        "extensions": [".js", ".jsx"]
      }
    ]
  ]
}
