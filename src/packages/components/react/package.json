    "@vizzuality/layer-manager-utils": "^1.0.4-alpha",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
