{
  "name": "@vizzuality/layer-manager-plugin-mapbox-gl",
  "version": "1.0.1-alpha",
  "keywords": [
    "layer-manager",
    "plugins",
    "mapbox-gl"
  ],
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "publish-alpha": "npm publish --tag=alpha",
    "prepare": "lerna run build --scope @vizzuality/layer-manager-plugin-mapbox-gl",
    "build": "bnr build",
    "watch": "rollup --config rollup.config.js --watch"
  },
  "betterScripts": {
    "build": {
      "command": "rollup -c",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@vizzuality/layer-manager-utils": "1.0.4-alpha"
  },
  "peerDependencies": {
    "deck.gl": "7.3.6",
    "lodash": "^4.17.20",
    "luma.gl": "7.3.2"
  }
}
