{
  "name": "layer-manager",
  "version": "0.1.4",
  "description": "A library to get a layer depending on provider and layer spec",
  "main": "dist/bundle.js",
  "scripts": {
    "serve": "http-server . -p 8000 -a 0.0.0.0",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "test": "mocha --require babel-core/register --require ./test/setup --recursive ./test/specs",
    "watch": "watchify src/index.js -o dist/bundle.js -t babelify -s layerManager --verbose",
    "build": "babel src --out-dir dist && browserify src/index.js -o dist/bundle.js -t babelify -s layerManager"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vizzuality/layer-manager.git"
  },
  "author": "Vizzuality",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.0",
    "chai": "^4.1.2",
    "eslint": "^4.9.0",
    "eslint-config-vizzuality": "^1.0.14",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "http-server": "^0.11.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.4",
    "watchify": "^3.10.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "leaflet": "^1.3.1",
    "lodash": "^4.17.5",
    "wri-json-api-serializer": "^1.0.1"
  },
  "peerDependencies": {
    "leaflet": "^1.3.1"
  },
  "engines": {
    "node": "8.11.x"
  }
}
