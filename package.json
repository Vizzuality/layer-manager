{
  "name": "layer-manager",
  "version": "1.0.12",
  "description": "A library to get a layer depending on provider and layer spec",
  "main": "dist/index.js",
  "scripts": {
    "serve": "http-server . -p 8000 -a 0.0.0.0",
    "prepublishOnly": "npm run build",
    "lint": "eslint ./src",
    "test": "mocha --require babel-core/register --require ./test/setup --recursive ./test/specs",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "prettier": "prettier --single-quote --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vizzuality/layer-manager.git"
  },
  "author": "Vizzuality",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-vizzuality": "^1.1.7",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.10.0",
    "http-server": "^0.11.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.4",
    "prettier": "^0.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "lodash": "^4.17.10",
    "wri-json-api-serializer": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "commitLimit": false
  }
}
